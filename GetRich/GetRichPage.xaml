<?xml version="1.0" encoding="utf-8"?>
<ContentPage 
  xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:local="clr-namespace:GetRich" x:Class="GetRich.GetRichPage">
  <ListView ItemsSource="{Binding Pictures}">
    <ListView.ItemTemplate>
      <DataTemplate>
        <ViewCell Height="320">
            <Image Source="{Binding Url}" />
        </ViewCell>
      </DataTemplate>
    </ListView.ItemTemplate>
  </ListView>
</ContentPage>
